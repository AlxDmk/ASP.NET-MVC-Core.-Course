{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using" : ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel" : "Warning",
    "WriteTo" : [
      {"Name" :  "Console"},
      {"Name" : "File", "Args": {"path": "logs/log.txt"}, "MinimumLevel": "Debug"}
    ],
    "Enrich": ["FromLogContext"]
  },
  "AllowedHosts": "*"
}
